{
    "name": "aozen/customizable-image-card",
    "description": "This package offers you customizable card with some options. Which includes: content area, image, redirect url, text color, size of image and sidebyside(image and text) option",
    "keywords": [
        "laravel",
        "nova",
        "card",
        "image",
        "title",
        "redirect",
        "custom",
        "customizable"
    ],
    "authors": [
        {
            "name": "Ali Ã–zen",
            "email": "aliozendev@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "^5.6",
        "laravel/nova": "*"
    },
    "autoload": {
        "psr-4": {
            "Aozen\\CustomizableImageCard\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aozen\\CustomizableImageCard\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
